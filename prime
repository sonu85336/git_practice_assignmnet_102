
function prime(number){

  let factor = 0;
  for(i=0;i<=number;i++){
    if(number%i==0){
           factor++
    }
  }
    if(factor==2){
      return true
    }else{
      return false
    }
    
  }
  
    
   
  let check = prime(15)
  if(check == true){
    console.log("Yes it is a prime number")
  }
  else if (check ==false){
    console.log("No it is not a prime number")
  }

  
